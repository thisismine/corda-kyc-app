<form class="form form-horizontal" role="form">
       <div class="form-group">
              <div class="col-md-4"></div>
       </div>
       <div class="form-group">
              <div class="col-md-4">
                     <h2>
                           <small><b>Search KYC User:</b></small>
                     </h2>
              </div>
       </div>
       <div class="form-group">
              <div class="col-md-10">
                     <label for="user_id"><b>User ID:</b></label>
              </div>
              <div class="col-md-4">
                     <!-- <input type="text" ng-model="itemDetail.UserName" placeholder="Search" class="form-control" id="user_id" /> -->
                     <input ng-model="searchUserId" placeholder="Search"
                           class="form-control">
              </div>
              <div class="col-md-4">
                     <button ng-click="search(searchUserId)" class="btn btn-info">
                           <span class="glyphicon glyphicon glyphicon-search" ng-click=""
                                  aria-hidden="true"></span>
                     </button>
              </div>
       </div>
</form>
 
<!---------------------------------Table---------------------------------------->
<div class="form-group">
       <div class="col-md-8">&nbsp;&nbsp;</div>
</div>
<div>
       <table class="table">
              <tr>
                     <!-- <th>Counter Party</th> -->
                     <th>User Name</th>
                     <th>User ID</th>
                     <th>Bank Name</th>
                     <th>Kyc Date</th>
                     <th>Kyc Expiry Date</th>
 
              </tr>
 
              <tr ng-repeat="item in items | filter:searchText:user_id">
                     <!-- <td>{{item.Counterparty}}</td> -->
 
                     <td>{{item.userName}}</td>
                     <td>{{item.userID}}</td>
                     <td>{{item.bankName}}</td>
                     <td>{{item.kycDate | date:'yyyy-MM-dd'}}</td>
                     <td>{{item.kYCExpiryDate | date:'yyyy-MM-dd'}}</td>
                     <td><div>
 
                                  <a id='dwnldLnk' download='kyc.pdf' style="display: none;" /> <a
                                         href="#/search" onclick="downloadPDF();" title='kyc.pdf'>Download Kyc Document</a>
 
                           </div></td>
                     <td><button ng-click="update()">Update</button></td>
 
              </tr>
       </table>
       <div ng-show="updateData">
              <form name="myForm" class="form form-horizontal" role="form">
                     <div class="form-group">
                           <div class="col-md-4">
                                  <label for="userID"><b>User ID:</b></label>
                           </div>
                           <div class="col-md-4">
                                  <input type="text" ng-model="updateDetail.userID" ng-readonly="true"
                                         placeholder="User ID" class="form-control" id="userID" />
                           </div>
                     </div>
 
                     <div class="form-group">
                           <div class="col-md-4">
                                  <label for="userName"><b>User Name:</b></label>
                           </div>
                           <div class="col-md-4">
                                  <input type="text" ng-model="updateDetail.userName"
                                         placeholder="User Name" class="form-control" id="userName">
                           </div>
                     </div>
                     <div class="form-group">
                           <div class="col-md-4">
                                  <label for="bankName"><b>Bank Name:</b></label>
                           </div>
                           <div class="col-md-4">
                                  <input type="text" ng-model="updateDetail.bankName"  readonly="readonly"
                                         placeholder="Bank Name" class="form-control" id="bankName">
                           </div>
                     </div>
                     <div class="form-group">
                           <div class="col-md-4">
                                  <label for="kycDate"><b>KYC Update date</b></label>
                     l      </div>
                           <div class="col-md-4">
                                  <input type="date"
                                         ng-model="updateDetail.kycUpdateDate" ng-readonly="true" class="form-control" id="kycDate">
                           </div>
                     </div>
                     <div class="form-group">
                           <div class="col-md-4">
                                  <label for="kycDate"><b>KYC Upload Document</b></label>
                           </div>
                           <div class="col-md-4">
                                  <input type="file" class="filestyle"  ng-model="fileUpload" data-buttonName="btn-default"
                                         data-size="nr" data-icon="false">
                                         <input type="button" name="" value="Upload" class="btn-default" ng-click=check()>
                           </div>
                     </div>
 
 
 
 
                     <div class="form-group">
                           <div class="col-md-1">
                                  <button type="submit" ng-click="callUpdate(updateDetail)"
                                         class="btn btn-primary">Submit</button>
 
 
                           </div>
              </form>
       </div>
 
</div>
 
 
 
 

